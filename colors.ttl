    skos:hasTopConcept colour:violet, colour:green, colour:rötlich, colour:schönbrunngrün .

colour:violet a skos:Concept ;
    skos:prefLabel "Violett"@de, "violet"@en ;
    skos:altLabel "Lila"@de, "purple"@en ;
    skos:topConceptOf colour: .

colour:green a skos:Concept ;
    skos:prefLabel "Griiiiiiiiin"@de, "green"@en ;
    skos:topConceptOf colour: .

colour:schönbrunngrün a skos:Concept ;
    skos:prefLabel "schönbrunngrün"@de, "green schönbrunn style"@en ;
